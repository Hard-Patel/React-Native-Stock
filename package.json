{
  "name": "RNChart",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios",
    "start": "npx react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "podinstall": "cd ios && pod install && cd ..",
    "postinstall": "patch-package; npx jetify; yarn run podinstall;",
    "postuninstall": "patch-package; npx jetify; yarn run podinstall;",
    "prettier": "prettier --write \"./src/**/*.{js,tsx,md,yml}\"",
    "format": "yarn run prettier",
    "format-check": "prettier --list-different \"./**/*.{js,tsx,md,yml}\"",
    "clean": "watchman watch-del-all && rm -rf package-lock.json && rm -rf node_modules && rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/haste-map-* && yarn install && say Project has been cleaned successfully",
    "clean-android": "cd android && ./gradlew clean && cd ../ && yarn add jetifier && npx jetify",
    "generate-icons": "./scripts/icon-generator/icon-generator.sh;",
    "dev-release": "concurrently npm:release:dev:*",
    "release:dev:android": "bundle exec fastlane android ",
    "release:dev:ios": "bundle exec fastlane ios ",
    "prepare": "husky install",
    "e2e:build-ios": "detox build --configuration ios",
    "e2e:test-ios": "detox test --configuration ios",
    "e2e:build-android": "detox build --configuration android",
    "e2e:test-android": "detox test --configuration android",
    "run:ios": "npx react-native run-ios --udid 4E83263C-0A37-48F4-8C47-842DE8CD2225",
    "install:android-apk": "cd android/app/release && adb install app-release.apk && cd ../../.. && adb shell am start -n com.intuions/com.intuions.MainActivity",
    "uninstall:android-apk": "adb uninstall com.intuions"
  },
  "dependencies": {
    "@pavelgric/react-native-theme-provider": "^0.2.0",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-clipboard/clipboard": "^1.13.2",
    "@react-native-community/netinfo": "^11.2.1",
    "@react-native-masked-view/masked-view": "^0.3.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.3.20",
    "babel-plugin-module-resolver": "^5.0.0",
    "gifted-charts-core": "^0.0.7",
    "metro-react-native-babel-preset": "^0.77.0",
    "moment": "^2.30.1",
    "native-base": "^3.4.28",
    "react": "18.2.0",
    "react-i18next": "^14.0.1",
    "react-native": "0.73.2",
    "react-native-actionsheet": "^2.4.2",
    "react-native-appstate-hook": "^1.0.6",
    "react-native-chart-kit": "^6.12.0",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.14.1",
    "react-native-gifted-charts": "^1.4.1",
    "react-native-image-picker": "^7.1.0",
    "react-native-image-progress": "^1.2.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-offline": "^6.0.2",
    "react-native-reanimated": "^3.6.1",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.29.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^14.1.0",
    "react-native-svg-transformer": "^1.3.0",
    "react-native-view-shot": "^3.8.0",
    "react-query": "^3.39.3",
    "rn-placeholder": "^3.0.3",
    "socket.io-client": "^4.7.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.19",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.3",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
